/*================ Small screen nav styles ================*/
@include at-query ($max, 1023px) {
  .nav-bar {padding: 0 10px;  background: rgba(10,148,171,0.95);
    .show-nav & {
      bottom: 0;   
      top: 90px;
    }
  }

  .show-nav,
  .show-nav body {
    /*     height: 100%; */
    overflow: hidden;
  }
}

/*================ Medium-down screen nav styles ================*/
@include at-query ($max, 1023px) {
  .nav-bar {
    position: fixed;

    z-index: 2000;
    @include transition(all 600ms cubic-bezier(0.47, 0.5, 0, 0.95));

    .show-nav & {
      max-height: 100%;
      overflow-y: scroll;
      -webkit-overflow-scrolling: touch;
      @include transition(all 450ms cubic-bezier(0.47, 0, 0, 0.95));
    }

    .wrapper {
      padding: 0;

      .show-nav & {
        min-height: 101%;
      }
    }

    .search-bar {
      margin: 0 $gutter/2 $gutter/2;
      width: auto;
      display: none;
      input[type="search"]::-webkit-input-placeholder {
        color: color-control($colorNav);
      }
      input::-webkit-input-placeholder {
        color: color-control($colorNav); 
      }
      .search-bar--submit .icon {
        color: color-control($colorNav);   
      }
    }  
  }
}  

/*================ Medium screen size only ================*/
@include at-query (null, $postSmall, $medium) {
  .nav-bar {
    .show-nav & {
      /*       top: $mobileNavBarHeight; */
      bottom: 0;
    }
  }
}

/*================ Large screen bar navigation ================*/
@include at-query ($min, 1024px) {
  .nav-bar {
    .search-bar {
      display: none;
    }
  }

  .site-nav {
    display: block;
  }
  .mobile-nav {
    display: none;
  }
}

/*================ Dropdowns ================*/
.icon-arrow-down {
  position: relative;
  top: -2px;
  font-size: 12px;

  .mobile-nav--dropdown & {
    @include transform(none);
  }

  .mobile-nav--has-dropdown & {
    top: 5px;right:7px;
    position: absolute;    width: 50px;
    text-align: right;
    font-size: 20px;
  }

  .site-nav--dropdown & {
    display: inline-block;
    line-height: 1;
    top: 0;
    @include transform(rotate(-90deg));
  }
}


.site-nav--dropdown,
.site-nav--dropdown-grandchild {
  position: absolute;
  top: 100%; text-align:left;
  left: 0;    padding: 10px 0 20px;
  border: 2px solid  $colorBody;
  background-color:rgba(10, 148, 171, .95);
  opacity: 0;
  pointer-events: none;
  z-index: 20;
  margin: 0;
  @include transition(all 150ms);

  .site-nav--dropdown--more & {
    top: -1px;
    left: auto;
    right: 100%;
  }

  .no-touch .site-nav--has-dropdown:hover > &,
  .site-nav--has-dropdown.nav-hover > &,
  .nav-focus + &,
  .nav-hover > & {
    opacity: 1;
    box-shadow: 0 5px 40px rgba(0,0,0,0.05);
    pointer-events: auto;
    @include transition(all 90ms);
  }

  a {
    text-transform: uppercase;
    padding: $gutter/3 $gutter*1.5 $gutter/3 $gutter - 5;

    &:hover,
    &:active,
    &:focus {
      background-color: #33a6b9;
    }

    &:focus {
    }
  }
  &:before {
    content: "";
    border-top: 5px solid #fff;
    width: 35%;
    display: block;
    position: absolute;
    top: -6px;
    left: -2px;
  }
}
.site-nav--has-dropdown:hover .site-nav--dropdown{opacity:1; pointer-events:auto;}
.site-nav--dropdown-grandchild {
  top: -1px;
  left: 100%;

  &.nav-outside {
    left: -100%;
  }
}

.mobile-nav {
  text-align: left;
  border-top: 1px solid lighten($colorNav, 10%);
  margin: 0;
  font-family: $accentFontStack;
  font-weight: $accentFontWeight;
  font-style: $accentFontStyle;
  font-size: {{ settings.type_navigation_size }};
  {% if settings.type_navigation_transform %}
  text-transform: uppercase;
  {% endif %}
  cursor: default;

  li {    position: relative;
    @include display-flexbox();
    @include justify-content(space-between);
    @include flex-wrap(wrap);
    border-bottom: 1px solid lighten($colorNav, 20%);
    -webkit-tap-highlight-color: rgba(255,255,255,0.2);
    margin: 0;

    &.mobile-nav--has-dropdown.nav-active {
      background-color: lighten($colorNav, 15%);
    }

    &.mobile-nav--has-dropdown-grandchild.nav-active {
      background-color: lighten($colorNav, 25%);
    }
  }

  a {
    @include flex(1);    text-transform: uppercase;
    display: block;
    text-decoration: none;    letter-spacing: 2px;
    padding:$gutter/3 $gutter/2;
  }
}

.mobile-nav--button {
  border-top: none;
  border-right: 1px solid lighten($colorNav, 20%);
  border-bottom: none;
  border-left: 1px solid lighten($colorNav, 20%);
  background: transparent;
  z-index: 10;
  padding: 0;
  display: inline-block;
  transition: transform;font-size: 20px;
  position: absolute;
  right: 12px;

  &.nav-active {
    @include transform(rotate(180deg)  translateX(-30px));
  }

  & > a {
    padding: 0;
  }

  &:before {
    color: $colorNavText;
  }
}

.mobile-nav--dropdown,
.mobile-nav--dropdown-grandchild {
  width: 100%;
  display: none;padding-left: 18px;padding-bottom: 30px;
  margin: 0;

  & > li {
    float: none;
    width: 100%;
    border: none;
    text-transform: none;
    border-top: 1px solid lighten($colorNav, 5%);
  }

  .mobile-nav--button {
    border-left: 1px solid lighten($colorNav, 5%);
    border-right: 1px solid lighten($colorNav, 5%);
  }
}

.mobile-nav--has-grandchildren a {
  padding-left: $gutter;
}

.mobile-nav--dropdown-grandchild a {
  padding-left: $gutter * 1.5;
}

.nav-active > .mobile-nav--dropdown,
.nav-active > .mobile-nav--dropdown-grandchild {
  display: block;
}

/*============================================================================
#Mobile Nav Bar
==============================================================================*/
#mobileNavBar {
  @include clearfix;
  display: none;
  width: 100%;
  background-color: $colorNav;

  .display-table-cell {
    padding: 0 23px;    position: relative;
    z-index: 11;
    &:first-child{float: left;}
    &:last-child{float: right;}
  }

  .cart-toggle {
    text-align: right;    margin-top: 4px;
  }

  /*================ Small mobile nav bar ================*/
  @include at-query ($max, 1023px) {
    position: absolute;
    top: 30px;    display: block;
    left: 0;
    right: 0;
  }

  /*================ Medium-down mobile nav bar ================*/
  @include at-query ($max, 1023px) {
    display: table;

    .show-nav & {
      /*       position: fixed;
      left: 0;
      top: 0; */
    }
  }
}
.menu-toggle.mobileNavBar-link {
    margin-top: 9px;
}
.mobileNavBar-link {
  display: block;
  padding: $gutter/3 0;
  color: $colorNavText;
  cursor: pointer;
  border: 0;
  background: none;

  &:hover,
  &:focus {
    color: $colorNavText;
  }

  &.menu-toggle {

    .icon {display: inline-block;position: relative;width:39px;border-top: 4px solid #fff;
      vertical-align: top;
      &:before{    height: 4px;
        width: 39px;
        background: #fff;
        content: "";
        position: absolute;
        bottom: -14px;
        left: 0;}
      &:after{    height: 4px;
        width: 25px;
        background: #fff;
        content: "";
        position: absolute;
        bottom: -28px;
        left: 0;}
    }
  }
}

@include at-query($max, $medium) {
  :target {
    padding-top: $mobileNavBarHeight;
  }
}

